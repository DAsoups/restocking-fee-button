<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
     <t t-name="pos_custom.ControlButtons" t-inherit="point_of_sale.ControlButtons" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[contains(@class, 'control-buttons')]" position="inside">
        <button class="control-button" t-on-click="onClickRestockingFee" style="border: none; border-radius: 3px">
                <span>Restocking Fee</span>
            </button>
        </xpath>
    </t>
</templates>